        <script><![CDATA[(function executeRule(current, previous /*null when async*/ ) {
    var rm = new sn_ws.RESTMessageV2('Notify External-Task', 'Send Task');
    rm.setStringParameterNoEscape('task_name', current.event_name);
    rm.setStringParameterNoEscape('due_date', current.event_date);
    rm.setStringParameterNoEscape('assigned_to', current.location);
	rm.setStringParameterNoEscape('event_id', current.event_id);

    var response = rm.execute();
 
    var responseBody = response.getBody();
    gs.info("Webhook Response: " + responseBody);
})(current, previous);]]></script>
